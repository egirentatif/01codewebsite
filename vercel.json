{
  "version": 2,
  "builds": [
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "buildEnv": {
    "NEXT_PUBLIC_THREEJS_URL": "https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js",
    "NEXT_PUBLIC_VANTA_URL": "https://cdn.jsdelivr.net/npm/vanta@0.5.24/dist/vanta.fog.min.js"
  },
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/((?!api|_next/static|_next/image|favicon.ico|three.min.js|vanta.fog.min.js).*)",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      }
    },
    {
      "src": "/_next/static/(chunks|css|js|media)/.+",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_THREEJS_URL": "https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js",
    "NEXT_PUBLIC_VANTA_URL": "https://cdn.jsdelivr.net/npm/vanta@0.5.24/dist/vanta.fog.min.js"
  }
}
